{
  "name": "markdown-previewer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-scripts": "1.0.10"
  },
  "scripts": {
    "start": "concurrently --kill-others \"npm run sass-watch\" \"react-scripts start\"",
