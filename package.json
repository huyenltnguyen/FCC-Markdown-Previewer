    "eslint-plugin-react": "^7.1.0",
    "gh-pages": "^1.0.0",
    "marked": "^0.3.6",
    "react-bootstrap": "^0.31.0",
    "react-media": "^1.6.1"
  },
  "homepage": "https://huyenltnguyen.github.io/FCC-Markdown-Previewer"
